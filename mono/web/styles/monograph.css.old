.reveal {
  overflow-y: visible;
  min-height: 100%;
}

.reveal:not(.overview) .slides .section:not(.present) {
  display: none !important;
}

.reveal:not(.overview) .slides .section:not(.stack) {
  padding: 145px 0 80px 0;
}

.reveal.overview .header,
.reveal.overview .footer {
